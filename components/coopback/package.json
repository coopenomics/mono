    "http-status": "^1.4.0",
    "ip": "^2.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "joi": "^17.13.1",
    "joi-to-typescript": "^4.13.0",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.1",
    "nunjucks": "^3.2.4",
