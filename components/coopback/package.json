    "@types/supertest": "^6.0.2",
    "@wharfkit/antelope": "^1.0.7",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "coopdoc-generator-ts": "workspace:*",
    "cooptypes": "workspace:*",
    "cors": "^2.8.5",
